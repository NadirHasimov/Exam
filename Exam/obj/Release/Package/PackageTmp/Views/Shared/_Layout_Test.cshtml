<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>

    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="~/assets/js/jquery-ui/css/no-theme/jquery-ui-1.10.3.custom.min.css">
    <link rel="stylesheet" href="~/assets/css/font-icons/entypo/css/entypo.css">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic">
    <link rel="stylesheet" href="~/assets/css/bootstrap.css">
    <link rel="stylesheet" href="~/assets/css/neon-core.css">
    <link rel="stylesheet" href="~/assets/css/neon-theme.css">
    <link rel="stylesheet" href="~/assets/css/neon-forms.css">
    <link rel="stylesheet" href="~/assets/css/custom.css" />
    <link href="~/Scripts/theme/jquery-ui/jquery-ui.css" rel="stylesheet" />

    @*<link href="~/NeonNew/css/neon-core.css" rel="stylesheet" />
        <link href="~/NeonNew/css/neon-theme.css" rel="stylesheet" />

        <link href="~/NeonNew/css/neon-forms.css" rel="stylesheet" />*@

    @*<link href="~/NeonNew/css/bootstrap.css" rel="stylesheet" />*@
    @*  <link href="~/Content/theme/font-icons/entypo/css/entypo.css" rel="stylesheet" />*@

    @RenderSection("Styles", required: false)

    @*<link href="~/Content/custom/css/common.css" rel="stylesheet" />*@

    <script src="~/Scripts/theme/jquery-1.11.3.min.js"></script>

    <script src="~/Scripts/theme/jquery.validate.min.js"></script>
    <script src="~/Scripts/theme/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/theme/jquery-ui/jquery-ui.min.js"></script>
</head>
<body class="page-body">
    @{
        int collapsed = 0;
        if (Request.Cookies["sidebarCollapsed"] != null)
        {
            collapsed = int.Parse(Request.Cookies["sidebarCollapsed"].Value);
        }
    }
    <div class="page-container @(collapsed == 1 ? "sidebar-collapsed" : "")">
        <div class="sidebar-menu fixed">
            <div class="sidebar-menu-inner ps-container ps-active-y">
                <header class="logo-env">
                    <!-- logo -->
                    <div class="logo">
                        <a href="https://demo.neontheme.com/dashboard/main/">
                            <img src="https://demo.neontheme.com/assets/images/logo@2x.png" width="120" alt="">
                        </a>
                    </div> <!-- logo collapse icon -->
                    <div class="sidebar-collapse">
                        <a href="#" class="sidebar-collapse-icon with-animation">
                            <!-- add class "with-animation" if you want sidebar to have animation during expanding/collapsing transition -->
                            <i class="entypo-menu"></i>
                        </a>

                    </div> <!-- open/close menu icon (do not remove if you want to enable menu on mobile devices) -->
                    <div class="sidebar-mobile-menu visible-xs">
                        <a href="#" class="with-animation">
                            <!-- add class "with-animation" to support animation -->
                            <i class="entypo-menu"></i>
                        </a>
                    </div>
                </header> <ul id="main-menu" class="main-menu" style="">
                    <li class="has-sub root-level">
                        <a href="https://demo.neontheme.com/dashboard/main/">
                            <i class="entypo-gauge"></i>
                            <span class="title">Menu</span>
                        </a>
                        <ul>
                            <li> <a href="https://demo.neontheme.com/dashboard/main/"><span class="title">Menu 1</span></a> </li>
                            <li> <a href="https://demo.neontheme.com/dashboard/main-2/"><span class="title">Menu 2</span></a> </li>
                            <li> <a href="https://demo.neontheme.com/dashboard/main-3/"><span class="title">Menu 3</span></a> </li>
                            <li class="has-sub"> <a href="https://demo.neontheme.com/skins/black/"><span class="title">Skins</span></a> <ul> <li> <a href="https://demo.neontheme.com/skins/black/"><span class="title">Black Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/white/"><span class="title">White Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/purple/"><span class="title">Purple Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/cafe/"><span class="title">Cafe Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/red/"><span class="title">Red Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/green/"><span class="title">Green Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/yellow/"><span class="title">Yellow Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/blue/"><span class="title">Blue Skin</span></a> </li> <li> <a href="https://demo.neontheme.com/skins/facebook/"><span class="title">Facebook Skin</span><span class="badge badge-secondary badge-roundless">New</span></a> </li> </ul> </li>
                            <li> <a href="https://demo.neontheme.com/dashboard/highlights/"><span class="title">What's New</span><span class="badge badge-success badge-roundless">v2.0</span></a> </li>
                        </ul>
                    </li>
                </ul> <div class="ps-scrollbar-x-rail" style="left: 0px; bottom: -192px;"><div class="ps-scrollbar-x" style="left: 0px; width: 0px;"></div></div><div class="ps-scrollbar-y-rail" style="top: 195px; height: 206px; right: 2px;"><div class="ps-scrollbar-y" style="top: 41px; height: 44px;"></div></div>
            </div>
        </div>
        <div class="main-content">
            @RenderBody()
        </div>
    </div>

    <script src="~/NeonNew/js/bootstrap.js"></script>
    <script src="~/NeonNew/js/joinable.js"></script>
    <script src="~/NeonNew/js/resizeable.js"></script>
    <script src="~/NeonNew/js/neon-api.js"></script>
    <script src="~/NeonNew/js/neon-custom.js"></script>
    <script src="~/NeonNew/js/neon-demo.js"></script>
    <script src="~/Scripts/theme/toastr.js"></script>
    <script src="~/NeonNew/js/TweenMax.min.js"></script>
    <script src="~/Scripts/theme/jquery.cookie.js"></script>
    <script src="~/Scripts/custom/common.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>