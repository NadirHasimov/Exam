@model Exam.Models.CandidateViewModel

@{
    ViewBag.Title = "List";
}

@section Scripts{
    <script src="~/Scripts/theme/jquery.dataTables.min.js"></script>
    @*<script src="~/assets/js/jquery.dataTables.min.js"></script>*@
    <script src="~/Scripts/theme/dataTables.jqueryui.js"></script>
    <script>
        $('#cand_list').DataTable();
    </script>
}
@section Styles{
    <link href="~/Content/Theme/css/dataTables.jqueryui.css" rel="stylesheet" />
}
<table class="table table-bordered table-striped " id="cand_list">
    <thead>
        <tr>
            <th>FIN code</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Middle name</th>
            <th>Birthdate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Candidates)
        {
            <tr>
                <td>@item.FinCode</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.MiddleName</td>
                <td>@item.Birthdate</td>
            </tr>
        }
    </tbody>
</table>